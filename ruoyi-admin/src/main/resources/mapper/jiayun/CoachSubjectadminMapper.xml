<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.jiayun.mapper.CoachSubjectadminMapper">
    
    <resultMap type="CoachSubjectadmin" id="CoachSubjectadminResult">
        <result property="id"    column="id"    />
        <result property="coachId"    column="coach_id"    />
        <result property="subjectId"    column="subject_id"    />
        <result property="createdAt"    column="created_at"    />
    </resultMap>

    <sql id="selectCoachSubjectadminVo">
        select id, coach_id, subject_id, created_at from coach_subject
    </sql>

    <select id="selectCoachSubjectadminList" parameterType="CoachSubjectadmin" resultMap="CoachSubjectadminResult">
        <include refid="selectCoachSubjectadminVo"/>
        <where>  
            <if test="coachId != null "> and coach_id = #{coachId}</if>
            <if test="subjectId != null "> and subject_id = #{subjectId}</if>
            <if test="createdAt != null "> and created_at = #{createdAt}</if>
        </where>
    </select>
    
    <select id="selectCoachSubjectadminById" parameterType="Long" resultMap="CoachSubjectadminResult">
        <include refid="selectCoachSubjectadminVo"/>
        where id = #{id}
    </select>

    <insert id="insertCoachSubjectadmin" parameterType="CoachSubjectadmin" useGeneratedKeys="true" keyProperty="id">
        insert into coach_subject
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="coachId != null">coach_id,</if>
            <if test="subjectId != null">subject_id,</if>
            <if test="createdAt != null">created_at,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="coachId != null">#{coachId},</if>
            <if test="subjectId != null">#{subjectId},</if>
            <if test="createdAt != null">#{createdAt},</if>
         </trim>
    </insert>

    <update id="updateCoachSubjectadmin" parameterType="CoachSubjectadmin">
        update coach_subject
        <trim prefix="SET" suffixOverrides=",">
            <if test="coachId != null">coach_id = #{coachId},</if>
            <if test="subjectId != null">subject_id = #{subjectId},</if>
            <if test="createdAt != null">created_at = #{createdAt},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCoachSubjectadminById" parameterType="Long">
        delete from coach_subject where id = #{id}
    </delete>

    <delete id="deleteCoachSubjectadminByIds" parameterType="String">
        delete from coach_subject where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>